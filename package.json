    "eslint-config-alloy": "^4.5.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-vue": "^8.5.0",
    "prettier": "^2.6.2",
